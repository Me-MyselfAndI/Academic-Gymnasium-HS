// ConsoleApplication18.cpp: определяет точку входа для консольного приложения.
//

#include <iostream>
using namespace std;

int main()
{
	int n, b;
	int a[100000];
	cin >> n;
	for(int i = 0; i < n; ++i) cin >> a[i];
	b = a[0] + a[4];
	for(int i = 0; i < n; ++i){
		for(int j = i; j < n; ++j){
			if(((j-i)>=4)&&(a[j] + a[i] < b)) b = a[i] + a[j];
		}
	}
	cout << b;
    return 0;
}
