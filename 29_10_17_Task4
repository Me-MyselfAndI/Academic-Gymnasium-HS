/** 
* Напишите программу находящую минимальную сумму пары элементов массива отстоящих друг от друга не менее чем 
* на 4 элемента(первый и пятый элемент могут подходить, а первый и третий - нет). Эффективность в данной программе не приоритетна.
**/

#include <iostream>
using namespace std;

int main()
{
	int n, b;
	int a[100000];
	cin >> n;
	for(int i = 0; i < n; ++i) cin >> a[i];
	b = a[0] + a[4];
	for(int i = 0; i < n; ++i){
		for(int j = i; j < n; ++j){
			if(((j-i)>=4)&&(a[j] + a[i] < b)) b = a[i] + a[j];
		}
	}
	cout << b;
    return 0;
}
